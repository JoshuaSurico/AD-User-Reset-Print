<Window x:Class="AD_User_Reset_Print.Views.TargetedADsList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AD_User_Reset_Print.Views"
        mc:Ignorable="d"
        Title="ADsList" Height="340" Width="500">
    <Grid x:Name="lbAD">
        <Label Content="Listes des ADs cibles" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold"/>
        <Button x:Name="btnAdd" Content="Ajouter" HorizontalAlignment="Left" Margin="10,52,0,0" VerticalAlignment="Top" FontSize="20" Width="140"/>
        <Button x:Name="btnRemove" Content="Enlever" HorizontalAlignment="Center" Margin="0,52,0,0" VerticalAlignment="Top" FontSize="20" Width="140"/>
        <Button x:Name="btnModify" Content="Modifier" HorizontalAlignment="Left" Margin="350,52,0,0" VerticalAlignment="Top" FontSize="20" Width="140"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,88,0,0" Width="480" Height="187" FontSize="20"/>
        <Button Content="Button" HorizontalAlignment="Center" Margin="0,280,0,0" VerticalAlignment="Top" FontSize="20" Width="480"/>
    </Grid>
</Window>
